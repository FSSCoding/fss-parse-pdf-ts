{
  "name": "fss-parse-pdf-ts",
  "version": "1.0.0",
  "description": "Professional TypeScript PDF parsing and manipulation toolkit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fss-parse-pdf-ts": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "pdf",
    "parser",
    "converter",
    "extractor",
    "cli",
    "typescript",
    "fss",
    "enterprise",
    "automation"
  ],
  "author": "FSS Coding",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FSSCoding/fss-parse-pdf-ts.git"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "ora": "^5.4.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  }
}
